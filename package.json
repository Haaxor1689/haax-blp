{
  "name": "haax-blp",
  "version": "1.1.1",
  "author": "Haaxor1689",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Haaxor1689/haax-blp.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "bundle": "bun build ./src/index.ts --compile --minify --outfile haax-blp/haax-blp && bun ./post-build.ts",
    "build": "tsup src/blp/index.ts --format esm,cjs --dts --minify --external sharp,jimp",
    "prepublish": "bun run build"
  },
  "devDependencies": {
    "@types/bun": "^1.3.2",
    "typescript": "^5.9.3",
    "tsup": "^6.7.0",
    "commander": "^11.1.0"
  },
  "dependencies": {
    "@haaxor1689/nil": "^1.0.3",
    "dxt-js": "^0.0.3",
    "rgbquant": "^1.1.2"
  },
  "optionalDependencies": {
    "sharp": "^0.34.5",
    "jimp": "^1.6.0"
  }
}
